<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="OptionView">
        <attr name="optionTitle" format="string" localization="suggested"/>
        <attr name="optionSubtitle" format="string"/>
        <attr name="optionIcon" format="reference"/>
    </declare-styleable>
    <declare-styleable name="PreviewView">
        <attr name="previewTitle" format="string" localization="suggested" />
        <attr name="previewActionName" format="string"/>
        <attr name="previewActionVisibility" format="enum">
            <enum name="gone" value="1"/>
            <enum name="visible" value="0"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="ShipmentView">
        <attr name="shipmentName" format="string" />
        <attr name="shipmentDesc" format="string"/>
        <attr name="shipmentIcon" format="reference"/>
    </declare-styleable>
    <declare-styleable name="StepView">
        <attr name="stepName" format="string" />
        <attr name="stepState" format="enum">
            <enum name="unselected" value="0"/>
            <enum name="selected" value="1"/>
            <enum name="done" value="2"/>
        </attr>
    </declare-styleable>
</resources>